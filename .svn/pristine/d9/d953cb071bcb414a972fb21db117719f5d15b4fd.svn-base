package cn.tedu.comic.entity;

import java.io.Serializable;
import java.util.Date;
/**
 * 书的图片
 * @author Administrator
 *
 */
public class Book implements Serializable{
	private static final long serialVersionUID = 1229074571874655205L;
	private Integer id;
	private String bookname;
	private String url;//书的本地路径
	private String intro;//书的简介
	private String type;//书的类型
	private String place;//书的产地
	private Integer isUse;//书是否被使用
	private String cover;//书的封面路径
	private Integer vip;//该书是否是vip书，0为不是，1为是
	private String createdUser;
	private Date createdTime;
	private String modifiedUser;
	private Date modifiedTime;
	private Integer redTicket;//红票
	private Integer blackTicket;//黑漂
	
	
	public String getIntro() {
		return intro;
	}
	public void setIntro(String intro) {
		this.intro = intro;
	}
	public Integer getRedTicket() {
		return redTicket;
	}
	public void setRedTicket(Integer redTicket) {
		this.redTicket = redTicket;
	}
	public Integer getBlackTicket() {
		return blackTicket;
	}
	public void setBlackTicket(Integer blackTicket) {
		this.blackTicket = blackTicket;
	}
	public String getCover() {
		return cover;
	}
	public void setCover(String cover) {
		this.cover = cover;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getBookname() {
		return bookname;
	}
	public void setBookname(String bookname) {
		this.bookname = bookname;
	}
	public String getUrl() {
		return url;
	}
	public void setUrl(String url) {
		this.url = url;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getPlace() {
		return place;
	}
	public void setPlace(String place) {
		this.place = place;
	}
	public Integer getIsUse() {
		return isUse;
	}
	public void setIsUse(Integer isUse) {
		this.isUse = isUse;
	}
	public String getCreatedUser() {
		return createdUser;
	}
	public void setCreatedUser(String createdUser) {
		this.createdUser = createdUser;
	}
	public Date getCreatedTime() {
		return createdTime;
	}
	public void setCreatedTime(Date createdTime) {
		this.createdTime = createdTime;
	}
	public String getModifiedUser() {
		return modifiedUser;
	}
	public void setModifiedUser(String modifiedUser) {
		this.modifiedUser = modifiedUser;
	}
	public Date getModifiedTime() {
		return modifiedTime;
	}
	public void setModifiedTime(Date modifiedTime) {
		this.modifiedTime = modifiedTime;
	}
	
	public Integer getVip() {
		return vip;
	}
	public void setVip(Integer vip) {
		this.vip = vip;
	}
	@Override
	public String toString() {
		return "Book [id=" + id + ", bookname=" + bookname + ", url=" + url + ", intro=" + intro + ", type=" + type
				+ ", place=" + place + ", isUse=" + isUse + ", cover=" + cover + ", vip=" + vip + ", createdUser="
				+ createdUser + ", createdTime=" + createdTime + ", modifiedUser=" + modifiedUser + ", modifiedTime="
				+ modifiedTime + ", redTicket=" + redTicket + ", blackTicket=" + blackTicket + "]";
	}
	
	
	
	
}
