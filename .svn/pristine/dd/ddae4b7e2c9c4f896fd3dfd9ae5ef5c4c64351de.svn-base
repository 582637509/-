package cn.tedu.comic.service.ex;

public class UpdateUserAboutException extends ServiceException {

	private static final long serialVersionUID = -7644472721271691190L;

	public UpdateUserAboutException() {
		super();
	}

	public UpdateUserAboutException(String message, Throwable cause, boolean enableSuppression,
			boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
	}

	public UpdateUserAboutException(String message, Throwable cause) {
		super(message, cause);
	}

	public UpdateUserAboutException(String message) {
		super(message);
	}

	public UpdateUserAboutException(Throwable cause) {
		super(cause);
	}

}
