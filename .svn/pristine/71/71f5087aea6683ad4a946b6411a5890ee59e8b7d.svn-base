#big-div {
	width: 980px;
	height: 1000px;
	background-repeat: no-repeat;
	background-position: center;
	margin: auto;
	margin-top: 60px;
	collapse:collapse;
}

#left-div {
	float: left;
	text-align: left;
	width: 229px;
	height: 580px;
	text-align: center;
	background-color: #f9f9f9;
	border:1px solid #ddd;
	clear: left;
	padding-top: 11px;
	overflow: hidden;
}

#right-div {
	width: 751px;
	float: left;
	height: 700px;
	border:1px solid #ddd;
	margin-left:-1px;
	padding: 18px;
	background-color: #e5f5e524;
}

#baocun {
	border-radius: 8px;
	margin: 5px auto;
}

.link {
    color: #000;
    position: relative;
    left: -1px;
    display: block;
    font-size: 16px;
    width:231px;
    height: 41px;
    line-height: 41px;
    padding: 0 0 0 35px;
    margin-left: -1px;
}
.tle{
	height:41px;
	width:227px;
	margin: 10px auto;
}
.tle:after {
    content: "";
    display: block;
    clear: both;
    height: 0;
    visibility: hidden;
}
.ff-icon{
	vertical-align: middle;
}
.prepr{
	height: 116px;
	float: left;
	overflow: hidden;
	margin:auto auto 10px 35px;
	position: relative;
}
.progress{
	width:200px;
	position: absolute;
	height:20px;
	left:35px;
	top:95px;
}
.menu-text  .ff-icon {
    color: #ddd;
}
.menu-text {
    display: inline-block;
    width: 133px;
    height: 41px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: left;
}
