@CHARSET "UTF-8";
#from_1 a img {
	height: 40px;
	margin-left: 30px;
}

.vip-tennis-condition{
	width: 229px; height: 139px; border: 1px solid yellow;
	float: left; margin: auto; margin-bottom: 20px;
	border: 1px solid #e8e8e8;background-image: url('../../image/personage/bei.png');
}
.menu-text{
    display: inline-block;
    width: 133px;
