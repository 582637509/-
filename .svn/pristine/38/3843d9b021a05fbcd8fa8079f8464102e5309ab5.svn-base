package cn.tedu.comic.entity;

import java.io.Serializable;

public class Words implements Serializable{
	
	private static final long serialVersionUID = 1219552667572153671L;
	private Integer id;
	private Integer Uid;
	private Integer UBid;
	private Integer bookid;
	private String words;
	private String time;
	public Integer getUid() {
		return Uid;
	}
	public void setUid(Integer uid) {
		Uid = uid;
	}
	public Integer getUBid() {
		return UBid;
	}
	public void setUBid(Integer uBid) {
		UBid = uBid;
	}
	public Integer getBookid() {
		return bookid;
	}
	public void setBookid(Integer bookid) {
		this.bookid = bookid;
	}
	
	public String getWords() {
		return words;
	}
	
	public void setWords(String words) {
		this.words = words;
	}
	
	public String getTime() {
		return time;
	}
	
	public void setTime(String time) {
		this.time = time;
	}
	
	public Integer getId() {
		return id;
	}
	
	@Override
	public String toString() {
		return "Words [id=" + id + ", Uid=" + Uid + ", UBid=" + UBid + ", bookid=" + bookid + ", words=" + words
				+ ", time=" + time + "]";
	}
}
